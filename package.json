  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cross-env": "^3.1.3",
    "envify": "^3.4.1",
    "mocha": "^3.0.2"
  },
  "dependencies": {
